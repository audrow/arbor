{
  "name": "arbor",
  "version": "0.1.0",
  "description": "A tool to make common ROS 2 operations easier",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src --ignore-path .gitignore --max-warnings 0",
    "prettier": "prettier --ignore-path .gitignore \"**/*.(js|jsx|json|yml|yaml|css|ts|tsx|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "test": "jest",
    "validate-sync": "npm-run-all lint check-format test",
    "validate": "npm-run-all --parallel lint check-format test"
  },
  "files": [
    "dist"
  ],
  "author": "Audrow Nash",
  "contributors": [
    "Audrow Nash <audrow@hey.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/audrow/node-js-template"
  },
  "keywords": [
    "nodejs",
    "template",
    "typescript"
  ],
  "homepage": "https://github.com/audrow/node-js-template",
  "bugs": {
    "mail": "audrow@hey.com",
    "url": "https://github.com/audrow/node-js-template/issues"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/git-clone": "^0.2.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.2",
    "@types/proper-url-join": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@types/git-rev-sync": "^2.0.0",
    "ajv": "^8.11.0",
    "commander": "^9.4.0",
    "git-clone": "^0.2.0",
    "git-rev-sync": "^3.0.2",
    "glob": "^8.0.3",
    "htmlparser2": "^8.0.1",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.2.9",
    "pino": "^8.2.0",
    "simple-git": "^3.10.0",
    "xml-formatter": "^2.6.1"
  }
}
